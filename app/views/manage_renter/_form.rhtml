<%= error_messages_for 'renter' %>
<!--[form:renter]-->
<p><label for="renter_name">Jmeno</label><br/>
<%= text_field 'renter', 'name'  %>*</p>

<p><label for="renter_Prijmeni">Prijmeni</label><br/>
<%= text_field 'renter', 'surname'  %>*</p>

<p><label for="renter_email">Email</label><br/>
<%= text_field 'renter', 'email'  %>*</p>

<p><label for="renter_telephone">Telefon</label><br/>
<%= text_field 'renter', 'telephone'  %>*</p>

<%= render :partial => 'renter_picture_upload', :object => @renter %>

