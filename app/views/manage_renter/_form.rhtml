<p><label for="renter_start_date"><%= get_string(:Start_date) %></label><br/>
	<%= date_select(:date, :start, {:order => [:day, :month, :year],
																	:use_month_numbers => true,
																	:default => {:day 	=> Time.at(@renter.start_date).day,
																							 :month => Time.at(@renter.start_date).month,
																							 :year 	=> Time.at(@renter.start_date).year } }) %>	</p>
<p><label for="renter_end_date">End date</label><br/>
	<%= date_select(:date, :end, {:order => [:day, :month, :year],
																	:use_month_numbers => true,
																	:default => {:day 	=> Time.at(@renter.end_date).day,
																							 :month => Time.at(@renter.end_date).month,
																							 :year 	=> Time.at(@renter.end_date).year } }) %>	</p>

